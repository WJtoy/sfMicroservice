  version: '0.0.1'
management:
  security:
    enabled: false
    roles: SUPERUSER
mybatis:
  type-aliases-package: com.kkl.kklplus.entity.sf
  mapper-locations: classpath*:/mapper/**/*Mapper.xml
  configuration:
    cache-enabled: true
    map-underscore-to-camel-case: true
logging:
